{
  "compilerOptions": {
    "target": "ES2023",
    "lib": ["ES2023", "DOM"],
    "types": ["cypress", "node", "cypress-file-upload"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "Node"
  },
  "include": [
    "cypress/**/*.ts",
    "node_modules/cypress"
  ]
}
